<% form_for @club, :html => {:multipart => true} do |f| %>
  <%=f.error_messages%>
  <p>
    <%=f.label t('user.name')%><br />
    <%=f.text_field :name%>
  </p>
  <p>
    <%=f.label t('user.creation_date')%><br />
    <%=f.date_select :creation_date%>
  </p>
  <p>
    <%=f.label t('user.club_icon')%><br />
    <%=f.file_field :club_icon%>
  </p>
  <p><%=f.submit t(:save_button)%></p>
<% end %>

