<% form_for @club do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label t('field_name') %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label t('field_creation_date') %><br />
    <%= f.date_select :creation_date %>
  </p>
  <p>
    <%= f.label t('field_club_icon') %><br />
    <%= f.file_field :club_icon %>
  </p>
  <p><%= f.submit t('save_button') %></p>
<% end %>

