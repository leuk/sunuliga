<div id = "clubs-#{club.id}">
<%=image_tag club.club_icon.url(:thumb)%>
<%=club.name%>
 <%= club.creation_date %>
  <%=link_to t('clubs.show_club') , club_path(club) , :title => t('clubs.show_club') %>
  <%=link_to t('clubs.edit_club') , edit_club_path(club.id) , :title => t('clubs.edit_club') %>
  <%=link_to t('clubs.delete_club') , club, :title => t('clubs.delete_club') , :confirm => t('confirm.delete'), :method => :delete%>
</div>

